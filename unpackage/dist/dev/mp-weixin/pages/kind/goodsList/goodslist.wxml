<view class="goods"><view class="head"><block wx:for="{{tabbarList}}" wx:for-item="items" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['itemChange',[index]]]]]}}" class="items" bindtap="__e"><view class="{{[index==current?'active':'']}}">{{items.name}}</view><block wx:if="{{index==1||index==2}}"><view class="pic"><uni-icons vue-id="{{'66119caf-1-'+index}}" type="top" size="8" color="{{items.status=='up'?'red':'#fff'}}" bind:__l="__l"></uni-icons><uni-icons vue-id="{{'66119caf-2-'+index}}" type="bottom" size="8" color="{{items.status=='down'?'red':'#fff'}}" bind:__l="__l"></uni-icons></view></block></view></block></view><view class="goods-list"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['goodsList','id',item.id,'id']]]]]]]}}" class="item" bindtap="__e"><image class="img" src="{{item.pic}}" mode></image><view class="title">{{item.name}}</view><view class="desc">{{item.price}}</view></view></block></view></view>